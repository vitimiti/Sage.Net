{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Sage.Net": "Debug"
    }
  },
  "Sage": {
    "Diagnostics": {
      "Dumps": {
        "Enabled": true,
        "DumpDirectory": "Dumps",
        "DumpLevel": "WithHeap",
        "MaxDumpFiles": 1,
        "FilePrefix": "sage_crash"
      }
    }
  }
}